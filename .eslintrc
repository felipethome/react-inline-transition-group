{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "constructor-super": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "jsx-quotes": [2, "prefer-double"],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "max-len": [2, 80, 4, {"ignoreUrls": true}],
    "new-cap": [2, {"newIsCap": true, "capIsNew": true}],
    "no-cond-assign": 2,
    "no-console": [1, {"allow": ["warn", "error"]}],
    "no-const-assign": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-multiple-empty-lines": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "object-curly-spacing": [2, "never"],
    "object-shorthand": 0,
    "one-var": [2, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "react/display-name": 2,
    "react/jsx-boolean-value": 2,
    "react/jsx-closing-bracket-location": 2,
    "react/jsx-curly-spacing": 2,
    "react/jsx-handler-names": [2, {"eventHandlerPrefix": "_?handle", "eventHandlerPropPrefix": "on"}],
    "react/jsx-indent-props": 0,
    "react/jsx-indent": [2, 2],
    "react/jsx-key": 2,
    "react/jsx-max-props-per-line": [2, {"maximum": 3}],
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/sort-prop-types": 2,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/no-danger": 2,
    "react/no-deprecated": 2,
    "react/no-did-update-set-state": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-multi-comp": 2,
    "react/no-string-refs": 2,
    "react/no-unknown-property": 2,
    "react/prop-types": 2,
    "react/require-extension": [2, {"extensions": [".js", ".jsx", ".json"]}],
    "react/self-closing-comp": 2,
    "react/sort-comp": 2,
    "react/wrap-multilines": [2, {"declaration": true, "assignment": true, "return": true}],
    "semi": 2,
    "space-infix-ops": 2,
    "space-unary-ops": [2, {"words": true, "nonwords": false}]
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "Promise": true
  }
}